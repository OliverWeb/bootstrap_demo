<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.migu.governance.mapper.MicroServiceMethodCheckedMapper">

	<insert id="insert" parameterType="MicroServiceMethodChecked">
		insert into micro_service_method_checked(servicenameandversion,methodname,beforeinfo,info,createtime) values(#{serviceNameAndVersion},#{methodName},#{beforeInfo},#{info},#{createtime})
	</insert>
	

	<select id="list"  resultType="MicroServiceMethodChecked">
		select id,servicenameandversion,methodname,beforeinfo,info,createtime from micro_service_method_checked order by createtime desc,servicenameandversion,methodname
	</select>
	
</mapper>  

